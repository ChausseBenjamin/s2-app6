def series_fct(freq1, type1, freq2, type2):
  # type 'low' ou 'high'
  b1, a1 = butterworth(freq1, type1)
  z1, p1, k1 = signal.tf2zpk(b1, a1)
  b2, a2 = butterworth(freq2, type2)
  z2, p2, k2 = signal.tf2zpk(b2, a2)
  zt, pt, kt = hp.seriestf(z1, p1, k1, z2, p2, k2)
  return zt, pt, kt
